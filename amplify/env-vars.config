# Core AWS Configuration
REACT_APP_AWS_REGION=eu-west-1
REACT_APP_AWS_PROJECT_REGION=${AWS_REGION}

# Rekognition Service Settings
REACT_APP_REKOGNITION_COLLECTION_ID=${REKOGNITION_COLLECTION_ID}
REACT_APP_REKOGNITION_THRESHOLD=95
REACT_APP_ID_ANALYSIS_ENABLED=true

# API Endpoints
REACT_APP_API_ENDPOINT=https://${API_GATEWAY_ID}.execute-api.${AWS_REGION}.amazonaws.com/prod
REACT_APP_CONFIG_API=/config

# Feature Flags
REACT_APP_FACE_VERIFICATION_ENABLED=true
REACT_APP_DOCUMENT_VERIFICATION_ENABLED=true

# Environment Specific
REACT_APP_ENV=${ENV}
REACT_APP_VERSION=${AWS_AMPLIFY_APP_VERSION}